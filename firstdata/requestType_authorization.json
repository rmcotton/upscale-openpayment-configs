{
  "id": 305,
  "pattern": "IFRAME",
  "requestType": "AUTHORIZATION",
  "destination": {
    "url": "https://checkout.globalgatewaye4.firstdata.com/payment",
    "method": "POST",
    "contentType": "application/x-www-form-urlencoded",
    "authenticationIds": [
      140
    ],
    "form": [
      {
        "name": "x_fp_hash",
        "value": "${auth.hmac.signature?lower_case}"
      },
      {
        "name": "x_fp_sequence",
        "value": "${auth.hmac.hmacFields.x_fp_sequence}"
      },
      {
        "name": "x_fp_timestamp",
        "value": "${auth.hmac.hmacFields.x_fp_timestamp}"
      },
      {
        "name": "x_amount",
        "value": "${auth.hmac.hmacFields.x_amount}"
      },
      {
        "name": "x_currency_code",
        "value": "${auth.hmac.hmacFields.x_currency_code}"
      }
    ]
  },
  "verify": {
    "localVerifyMapping": {
      "mappings": [
        {
          "from": "authResult",
          "to": "result",
          "map": {
            "AUTHORIZED": "AUTHORIZED",
            "CANCELLED": "CANCELLED"
          },
          "fixed": false,
          "isCustom": false
        }
      ]
    }
  }
}

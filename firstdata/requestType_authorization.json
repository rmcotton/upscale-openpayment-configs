{
  "id": 305,
  "pattern": "IFRAME",
  "requestType": "AUTHORIZATION",
  "destination": {
    "url": "https://demo.globalgatewaye4.firstdata.com/payment",
    "method": "POST",
    "contentType": "application/x-www-form-urlencoded",
    "authenticationIds": [
      168
    ],
    "form": [
      {
        "name": "x_amount",
        "value": "${auth.hmac.hmacFields.x_amount}"
      },
      {
        "name": "x_currency_code",
        "value": "${auth.hmac.hmacFields.x_currency_code}"
      },
      {
        "name": "x_show_form",
        "value": "PAYMENT_FORM"
      },
      {
        "name": "x_login",
        "value": "${vars.x_login}"
      },
      {
        "name": "x_fp_hash",
        "value": "${auth.hmac.signature?lower_case}"
      },
      {
        "name": "x_fp_sequence",
        "value": "${auth.hmac.hmacFields.x_fp_sequence}"
      },
      {
        "name": "x_fp_timestamp",
        "value": "${auth.hmac.hmacFields.x_fp_timestamp}"
      },
      {
        "name" : "x_type",
        "value" : "AUTH_ONLY"
      },
      {
        "name" : "x_receipt_link_method",
        "value" : "AUTO-GET"
      },
      {
        "name" : "x_receipt_link_text",
        "value" : "Return to Upscale"
      },
      {
        "name" : "x_receipt_link_url",
        "value" : "https://www.example.com/"
      }
    ]
  },
  "verify": {
    "localVerifyMapping": {
      "mappings": [
        {
          "from": "x_response_code",
          "to": "result",
          "map": {
            "1": "AUTHORIZED",
            "2": "UNAUTHORIZED"
          },
          "fixed": false,
          "isCustom": false
        },
        {
          "from" : "x_SHA1_Hash",
          "to" : "x_SHA1_Hash",
          "fixed": false,
          "isCustom": true
        },
        {
          "from" : "x_fp_sequence",
          "to" : "x_fp_sequence",
          "fixed": false,
          "isCustom": true
        },
        {
          "from" : "x_fp_timestamp",
          "to" : "x_fp_timestamp",
          "fixed": false,
          "isCustom": true
        },
        {
          "from" : "x_currency_code",
          "to" : "x_currency_code",
          "fixed": false,
          "isCustom": true
        },
        {
          "from" : "x_amount",
          "to" : "x_amount",
          "fixed": false,
          "isCustom": true
        }
      ]
    }
  }
}
